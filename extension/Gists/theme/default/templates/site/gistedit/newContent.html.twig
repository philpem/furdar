{% extends 'site/page.html.twig' %}

{% block pageTitle %}Edit {{  gist.title }} - {% endblock %}

{% block htmlHeader %}
	<meta name="openacalendar:gist:slug" content="{{  gist.slug }}" />
	<link rel="stylesheet" type="text/css" href="/theme/default/css/gists.css?v={{ config.assetsVersion }}">
{% endblock %}

{% block breadcrumbs %}
	{%  include 'site/breadcrumbs.html.twig' with { 'breadcrumbs': [  { 'url':'/gist/' ~ gist.slug, 'label':'gist' },  { 'url':'/gist/' ~ gist.slug ~ '/edit', 'label':'edit' },  { 'url':'/gist/' ~ gist.slug ~ '/edit/content/new', 'label':'new' } ]  } %}
{% endblock %}

{% block content %}


	<div class="contentMain">

		<form action="#" method="post" class="styled">

			{{ form_errors(form) }}

			{{ form_rest(form) }}

			<div class="bigSubmitActionWrapper"><input type="submit"  value="Add" class="bigSubmitAction"/></div>
			<div class="afterBigSubmitActionWrapper"></div>
		</form>

	</div>

{% endblock %}


{% extends 'site/page.html.twig' %}

{% block pageTitle %}History - {% endblock %} 

{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
{% endblock %}

{% block breadcrumbs %}
	{%  include 'site/breadcrumbs.html.twig' with { 'breadcrumbs': [  { 'url':'/history', 'label':'History of changes' } ]  } %}
{% endblock %}


{% block content %}

	<ul class="history">
		{% for historyItem in historyItems %}
			{%  if historyItem.siteWebTemplate %}
				{%  include historyItem.siteWebTemplate with { 'historyItem':historyItem } %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}

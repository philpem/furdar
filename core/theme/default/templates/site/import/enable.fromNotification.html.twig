{% extends 'site/pageMinimal.html.twig' %}


{% block pageTitle %}Enable {{ import.title }} - {% endblock %}

{% block content %}

	<div class="contentMain">

		<h2>Enable Import {{  import.title }}</h2>

		<form action="#" method="post" class="styled">
			<input type="hidden" name="CSFRToken" value="{{ getCSFRToken() }}">
			<input type="hidden" name="enable" value="yes">

			<div class="bigSubmitActionWrapper"><input type="submit"  value="Enable" class="bigSubmitAction"/></div>
			<div class="afterBigSubmitActionWrapper"></div>
		</form>

	</div>

{% endblock %}



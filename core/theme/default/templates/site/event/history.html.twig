{% extends 'site/event/page.html.twig' %}


{% block pageTitle %}History of changes of {{ event.summaryDisplay }} {%  if event.isCancelled  %}[CANCELLED] {%  endif %}- {% endblock %}


{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
	{{ parent() }}
{% endblock %}
	
{% block tabHistory %}currentTab{% endblock %}

{% block innerContent %}


	<ul class="history">
		{% for historyItem in eventHistories %}
			{%  if historyItem.siteWebTemplate %}
				{%  include historyItem.siteWebTemplate with { 'historyItem':historyItem, 'eventRollbackLink':true , 'eventRollbackLinkEvent':event} %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}


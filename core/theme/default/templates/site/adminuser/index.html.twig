{% extends 'site/page.html.twig' %}


{% block pageTitle %}User Admin - {% endblock %}


{% block breadcrumbs %}
	{%  include 'site/breadcrumbs.html.twig' with { 'breadcrumbs': [  { 'url':'/admin', 'label':'admin' },  { 'url':'/admin/user', 'label':'users' }, { 'url':'/admin/user/'~user.username, 'label':user.username} ]  } %}
{% endblock %}

{% block content %}


	<div class="contentMain">

		<h2>User {{ user.username }}</h2>
		
		<h3>Has User Permissions</h3>

		<table class="data">
			<tr>
				<th>Extension</th>
				<th>ID</th>
			</tr>
			{% for userpermission in userpermissions %}
				<tr>
					<td>{{ userpermission.userPermissionExtensionID }}</td>
					<td>{{ userpermission.userPermissionKey }}</td>
				</tr>
			{% endfor %}
		</table>

	</div>



		
{% endblock %}

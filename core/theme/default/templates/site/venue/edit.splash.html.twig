{% extends 'site/venue/page.html.twig' %}


{% block pageTitle %}Edit {{ venue.title }} - {% endblock %}

{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
	{{ parent() }}
{% endblock %}

{% block innerContent %}

	<ul class="actionList">


		{%  if currentUserActions.has("org.openacalendar","venueEditDetails") %}
			<li><a href="/venue/{{ venue.slugForUrl }}/edit/details"><div class="fa fa-pencil"></div> Edit Details</a></li>
		{%  endif %}
		{%  if currentUserActions.has("org.openacalendar","venueEditMedia") %}
			<li><a href="/venue/{{ venue.slugForUrl }}/edit/media"><div class="fa fa-pencil"></div> Edit Media</a></li>
		{%  endif %}
	</ul>

{% endblock %}


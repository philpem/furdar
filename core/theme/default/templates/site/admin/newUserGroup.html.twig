{% extends 'site/page.html.twig' %}


{% block pageTitle %}User Groups Admin - {% endblock %}

{% block breadcrumbs %}
	{%  include 'site/breadcrumbs.html.twig' with { 'breadcrumbs': [  { 'url':'/admin', 'label':'admin' },  { 'url':'/admin/usergroup', 'label':'user groups' } , {  'url':'/admin/usergroup/new', 'label':'new' } ]  } %}
{% endblock %}

{% block content %}

	<div class="contentMain">

		<form action="#" method="post" class="styled">

			{{ form_errors(form) }}

			{{ form_row(form.title) }}
			{{ form_row(form.description) }}
			{{ form_rest(form) }}

			<div class="bigSubmitActionWrapper"><input type="submit"  value="Save" class="bigSubmitAction"/></div>
			<div class="afterBigSubmitActionWrapper"></div>
		</form>

	</div>

{% endblock %}


{% extends 'site/group/page.mainTab.html.twig' %}

{% block pageTitle %}{{ group.title }} - {% endblock %}


{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
	{{ parent() }}
{% endblock %}

{% block tabDetails %}currentTab{% endblock %}

{% block switcherCalendar %}current{% endblock %}

{% block eventsContent %}


	{%  include '/site/common/calendar.monthly.html.twig' with {'showCurrentUserOptions':true, 'addURL':'/event/new?group=' ~ group.slug, 'calURL':'/group/' ~ group.slugForUrl ~ '/calendar' } %}


{% endblock %}

{% block scriptsForState %}
	{# % include '/site/common/mediasJavaScriptInformation.html.twig' with { medias:medias } % #}
	{{ parent() }}
{% endblock %}

{% block scripts %}
	{{ parent() }}
	<script type="text/javascript">
		var showCurrentUserOptions = {% if currentUser is not null %}true{% else %}false{% endif %};
	</script>
	<script src="/theme/default/js/calendar.site.js?v={{ config.assetsVersion }}" type="text/javascript"></script>
{% endblock %}


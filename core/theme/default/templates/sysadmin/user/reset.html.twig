{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}User {{  user.username }} Resets - {% endblock %}

{% block content %}


	<table class="data">
		<tr>
			<th>Created</th>
			<th>Reset<th>
		</tr>
		{% for reset in resets %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ reset.getCreatedAt  |date('D H:i jS M Y' , currentTimeZone) }}</td>
				<td>
					{% if reset.getResetAt %}
						{{ reset.getResetAt  |date('D H:i jS M Y' , currentTimeZone)}}
					{% endif %}
				</td>				
				
			</tr>
		{% endfor %}
	</table>

{% endblock %}

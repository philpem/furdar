{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}User {{  user.username }} Verify - {% endblock %}

{% block content %}


	<table class="data">
		<tr>
			<th>Created</th>
			<th>Verified</th>
			<th>Verified From IP</th>
		</tr>
		{% for verify in verifies %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ verify.getCreatedAt  |date('D H:i jS M Y' , currentTimeZone) }}</td>
				<td>
					{% if verify.getVerifiedAt %}
						{{ verify.getVerifiedAt  |date('D H:i jS M Y', currentTimeZone )}}
					{% endif %}
				</td>
				<td>
					{{  verify.verifiedFromIP }}
				</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}

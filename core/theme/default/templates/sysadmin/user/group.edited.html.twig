{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Groups Edited By User - {% endblock %}

{% block content %}

	<table class="data">
		<tr>
			<th>Slug</th>
			<th>Title</th>
			<th>&nbsp;</th>
		</tr>
		{% for group in groups %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ group.slug }}</td>
				<td>{{ group.title }}</td>
				<td>
					<a href="/sysadmin/site/{{ group.siteId }}/group/{{ group.slug }}" class="button">Show</a>
				</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}


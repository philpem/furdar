{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Config - {% endblock %}

{% block content %}

	<table class="oneData">
		<tr>
			<th>piwikServerHTTP</th>
			<td>{{ config.piwikServerHTTP }}
				{% include 'sysadmin/config/index.configcheck.html.twig' with {'field':'piwikServerHTTP'} %}
			</td>
		</tr>
		<tr>
			<th>piwikServerHTTPS</th>
			<td>{{ config.piwikServerHTTPS }}
				{% include 'sysadmin/config/index.configcheck.html.twig' with {'field':'piwikServerHTTPS'} %}
			</td>
		</tr>
		<tr>
			<th>piwikSiteID</th>
			<td>{{ config.piwikSiteID }}
				{% include 'sysadmin/config/index.configcheck.html.twig' with {'field':'piwikSiteID'} %}
			</td>
		</tr>
		<tr>
			<th>googleAnalyticsTracking</th>
			<td>{{ config.googleAnalyticsTracking }}
				{% include 'sysadmin/config/index.configcheck.html.twig' with {'field':'googleAnalyticsTracking'} %}
			</td>
		</tr>
	</table>

{% endblock %}


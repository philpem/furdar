{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Site Quota {{ sitequota.title }} - {% endblock %}

{% block content %}


	<table class="oneData">
		<tr>
			<th>ID</th>
			<td>{{ sitequota.id }}</td>
		</tr>
		<tr>
			<th>Title</th>
			<td>{{ sitequota.title }}</td>
		</tr>
		<tr>
			<th>Code</th>
			<td>{{ sitequota.code }}</td>
		</tr>
		<tr>
			<th>Max New Events Per Month</th>
			<td>{{ sitequota.maxNewEventsPerMonth }}</td>
		</tr>
		<tr>
			<th>Max New Groups Per Month</th>
			<td>{{ sitequota.maxNewGroupsPerMonth }}</td>
		</tr>
		<tr>
			<th>Max New Venues Per Month</th>
			<td>{{ sitequota.maxNewVenuesPerMonth }}</td>
		</tr>
		<tr>
			<th>Max Countries</th>
			<td>{{ sitequota.maxCountries }}</td>
		</tr>
		<tr>
			<th>Max Media MB</th>
			<td>{{ sitequota.maxMediaMB }}</td>
		</tr>
	</table>



{% endblock %}
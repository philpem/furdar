{% extends 'sysadmin/page.html.twig' %}

{% block content %}

	<p>Series</p>

	<ul class="menuOnPage">
		{%  for report in reportsSeries %}
			<li>
				<a href="/sysadmin/report/series/{{  report.extensionID }}/{{  report.reportId }}/">{{  report.reportTitle }}</a>
				{%  if report.getHasFilterTime %}
					(<a href="/sysadmin/report/seriesbytime/{{  report.extensionID }}/{{  report.reportId }}/">By Time</a>)
				{%  endif %}
			</li>
		{%  endfor %}
	</ul>

	<p>Value</p>

	<ul class="menuOnPage">
		{%  for report in reportsValue %}
			<li>
				<a href="/sysadmin/report/value/{{  report.extensionID }}/{{  report.reportId }}/">{{  report.reportTitle }}</a>
				{%  if report.getHasFilterTime %}
					(<a href="/sysadmin/report/valuebytime/{{  report.extensionID }}/{{  report.reportId }}/">By Time</a>)
				{%  endif %}
			</li>
		{%  endfor %}
	</ul>



{% endblock %}



{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Incoming Links - {% endblock %}

{% block breadcrumbs %}
	<li><a href="/sysadmin/site/{{ site.id }}/">{{ site.title }}</a></li>
	<li class="current"><a href="/sysadmin/site/{{ site.id }}/incominglink">incoming links</a></li>
{% endblock %}

{% block content %}

	<table class="data">
		<tr>
			<th>ID</th>
			<th>Source</th>
			<th>Target</th>
			<th>Created</th>
			<th>&nbsp;</th>
		</tr>
		{% for incominglink in incominglinks %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ incominglink.id }}</td>
				<td>{{ incominglink.sourceURL }}</td>
				<td>{{ incominglink.targetURL }}</td>
				<td>
					<!-- <a href="/sysadmin/site/{{ site.id }}/incominglink/{{ incominglink.id }}" class="button">Show</a> -->
				</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}


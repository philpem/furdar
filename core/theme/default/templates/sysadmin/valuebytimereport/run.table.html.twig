{% extends 'sysadmin/page.html.twig' %}

{% block content %}

	<h2>{{  report.reportTitle }} By Time</h2>

	 <table class="data">
		 <tr>
			 <td>Start</td>
			 <td>{%  if startAt %}{{  startAt | date('Y-m-d H:i:s', 'Europe/London')  }}{%  endif %}</td>
		 </tr>
		 <tr>
			 <td>End</td>
			 <td>{%  if endAt %}{{  endAt | date('Y-m-d H:i:s', 'Europe/London')  }}{%  endif %}</td>
		 </tr>
		 <tr>
			 <td>Site ID</td>
			 <td>{%  if filterSiteId %}{{  filterSiteId  }}{%  endif %}</td>
		 </tr>
	 </table>


	<table class="data">
		<tr>
			<th>Label</th>
			<th>Data</th>
		</tr>
		{% for data in reportData %}
			<tr>
				<td>
					{{  data.labelText }}
				</td>
				<td>
					{{  data.data }}
				</td>
			</tr>
		{%  endfor %}
	</table>

{% endblock %}



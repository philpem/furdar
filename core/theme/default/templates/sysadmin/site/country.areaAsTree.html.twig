<li class="area">
	<span class="content{% if areaData.area.cachedFutureEvents %}WithEvents{% else %}WithNoEvents{% endif %}">
		{{ areaData.area.title }} {% if areaData.area.isDeleted %}[DELETED]{% endif %}
		{% if areaData.area.cachedFutureEvents %}
			<span title="{{areaData.area.cachedFutureEvents}} future events"> ({{areaData.area.cachedFutureEvents}})</span>
		{% endif %}		
	</span>
	<ul class="areasAdmin">
		{% for areaChild in areaData.children %}
			{% include 'sysadmin/site/country.areaAsTree.html.twig' with {'areaData':areaChild} %}
		{% endfor %}
	</ul>
</li>



{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Site {{  site.title }} Countries - {% endblock %}

{% block breadcrumbs %}
	<li><a href="/sysadmin/site/{{ site.id }}/">{{ site.title }}</a></li>
	<li class="current"><a href="/sysadmin/site/{{ site.id }}/usernoteditor">users not editors</a></li>
{% endblock %}


{% block content %}

	{%  if featureActive %}
		<p>This feature is not active in Single Site Mode!</p>
	{%  endif %}

	<table class="data">
		<tr>
			<th>ID</th>
			<th>Username</th>
		</tr>
		{% for user in users %}
			<tr {% if loop.index is even %}class="alternative"{% endif %}>
				<td><a href="/sysadmin/user/{{ user.id }}">{{ user.id }}</a></td>
				<td><a href="/sysadmin/user/{{ user.id }}">{{ user.username }}</a></td>
			</tr>
		{% endfor %}
	</table>


{% endblock %}


